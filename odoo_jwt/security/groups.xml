<odoo>
    <data>
        <record model="res.groups" id="group_jwt_user">
            <field name="name">JWT User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
        </record>
        <record model="res.groups" id="group_jwt_manager">
            <field name="name">JWT Manager</field>
            <field name="implied_ids" eval="[(4, ref('group_jwt_user'))]" />
        </record>
        <record id="base.group_system" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('group_jwt_manager'))]" />
        </record>
    </data>
</odoo>